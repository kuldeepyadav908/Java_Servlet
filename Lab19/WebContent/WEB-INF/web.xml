  </servlet>
  
  <servlet-mapping>
  <servlet-name>ServletB</servlet-name>
  <url-pattern>/b.jlc</url-pattern>
  </servlet-mapping>
  
  <servlet>
  <servlet-name>ServletC</servlet-name>
