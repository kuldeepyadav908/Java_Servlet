<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <welcome-file-list>
  	<welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  
  <servlet>
  <servlet-name>SearchServlet</servlet-name>
  <servlet-class>com.coursecube.servlet.SearchBookServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
  <servlet-name>SearchServlet</servlet-name>
  <url-pattern>/searchbook.jlc</url-pattern>
  </servlet-mapping>
  
  <servlet>
  <servlet-name>AddToCartServlet</servlet-name>
  <servlet-class>com.coursecube.servlet.AddToCartServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
  <servlet-name>AddToCartServlet</servlet-name>
  <url-pattern>/addtocart.jlc</url-pattern>
  </servlet-mapping>
  
  <servlet>
  <servlet-name>ShowCartServlet</servlet-name>
  <servlet-class>com.coursecube.servlet.ShowCartServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
  <servlet-name>ShowCartServlet</servlet-name>
  <url-pattern>/showcart.jlc</url-pattern>
  </servlet-mapping>
  
  <servlet>
  <servlet-name>RemoveFromCart</servlet-name>
  <servlet-class>com.coursecube.servlet.RemoveFromCartServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
  <servlet-name>RemoveFromCart</servlet-name>
  <url-pattern>/removefromcart.jlc</url-pattern>
  </servlet-mapping>
</web-app>